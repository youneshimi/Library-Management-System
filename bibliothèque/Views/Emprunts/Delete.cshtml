@model Emprunt

@{
    ViewData["Title"] = "Delete";
}




<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Emprunt</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label class="control-label">Livre ID</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LivreId)
        </dd>
        <dt class="col-sm-2">
            <label class="control-label">Abonne ID</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AbonneId)
        </dd>
        <dt class="col-sm-2">
            <label class="control-label">Date d'Emprunt</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEmprunt)
        </dd>
        <dt class="col-sm-2">
            <label class="control-label">Date de Retour</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateRetour)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <div class="form-group">
            <button type="submit" class="btn btn-danger">Delete</button>
        </div>
        <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    </form>
</div>
