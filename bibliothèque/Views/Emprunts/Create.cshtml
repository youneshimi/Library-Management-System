@model Emprunt

@{
    ViewData["Title"] = "Create";
}




<h1>Create</h1>

<h4>Emprunt</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="LivreId" class="control-label">Livre ID</label>
                <input asp-for="LivreId" class="form-control" />
                <span asp-validation-for="LivreId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AbonneId" class="control-label">Abonne ID</label>
                <input asp-for="AbonneId" class="form-control" />
                <span asp-validation-for="AbonneId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateEmprunt" class="control-label">Date d'Emprunt</label>
                <input asp-for="DateEmprunt" class="form-control" />
                <span asp-validation-for="DateEmprunt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateRetour" class="control-label">Date de Retour</label>
                <input asp-for="DateRetour" class="form-control" />
                <span asp-validation-for="DateRetour" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Create</button>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
